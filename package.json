{
  "name": "DocumentReader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clear": "cd android && gradlew clean & cd../",
    "prebuild": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev false --reset-cache --assets-dest android/app/src/main/res/",
    "build": "cd android && gradlew assembleDebug && cd../",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ra": "rm -rf android/app/src/main/res/drawable-hdpi &&  rm -rf android/app/src/main/res/drawable-mdpi &&  rm -rf android/app/src/main/res/drawable-xhdpi && rm -rf android/app/src/main/res/drawable-xxhdpi && rm -rf android/app/src/main/res/drawable-xxxhdpi && rm -rf android/app/src/main/res/raw"
  },
  "dependencies": {
    "@eva-design/eva": "^2.1.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@regulaforensics/react-native-document-reader-api": "^5.6.0",
    "@regulaforensics/react-native-document-reader-core-fullrfid": "^5.6.0",
    "@regulaforensics/react-native-face-api-beta": "0.0.30",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "@ui-kitten/components": "^5.1.0",
    "@ui-kitten/eva-icons": "^5.1.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-check-box": "^2.1.7",
    "react-native-dl-api": "^1.0.26",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.0.5",
    "react-native-progress": "^4.1.2",
    "react-native-radio-buttons-group": "^1.0.7",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-native-customized-image-picker": "^1.2.2",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
